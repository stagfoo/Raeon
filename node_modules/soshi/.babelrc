{
  "passPerPreset": true,
  "presets": [
    {
      "passPerPreset": false,
      "presets": [
        "env",
        "es2015",
        "react",
        {
          "plugins": [
            "babel-plugin-add-module-exports",
            "transform-es2015-destructuring",
            "transform-object-rest-spread",
            "transform-class-properties",
            "transform-flow-comments",
            [
              "transform-react-jsx"
            ],
            [
              "transform-runtime",
              {
                "polyfill": true,
                "regenerator": true
              }
            ]
          ]
        }
      ]
    },
    [
      "env",
      {
        "targets": {
          "Android": 4
        }
      }
    ]
  ]
}
